{
    "id": "{{timestamp}}",
    "date": "{{env `yearmonthday`}}",
    "iso": "archlinux-{{user `date`}}-x86_64.iso",
    "headless": "true",
    "stage": "bootstrap",
    "stage_files": "bootstrap,bootstrap-partition,bootstrap-skel",
    "stage_cmd": "--auto /dev/sda \"\" toor user resu --no-graphical-env",
    "prov_user": "root",
    "prov_pass": "toor",
    "ovf_name": "../archlinux-{{user `stage`}}-console-{{user `date`}}-x86_64",
    "vbox_name": "{{user `id`}}",
    "mem_build": "2048",
    "mem_after": "256",
    "cpu_build": "2",
    "cpu_after": "1",
    "bridge_adapter": "none",
    "localmirror": ""
}